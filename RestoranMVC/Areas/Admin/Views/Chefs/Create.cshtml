@using RestoranMVC.Models
@using RestoranMVC.ViewModels
@model ChefsVm
<div  class="container">
    <form method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label asp-for="Name">Name</label>
            <input asp-for="Name" class="form-control" placeholder="Name">
            <span asp-validation-for="Name"></span>
        </div>
        <div class="form-group">
            <label asp-for="Position">Position</label>
            <input asp-for="Position" class="form-control" placeholder="Name">
            <span asp-validation-for="Position"></span>
        </div>
        <div class="form-group">
            <label asp-for="file">Image</label>
            <input asp-for="file" class="form-control" placeholder="file">
            <span asp-validation-for="file"></span>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>